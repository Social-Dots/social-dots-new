{
    "version": 2,
    "builds": [
      {
        "src": "vercel_app.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "50mb"
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/vercel_app.py"
      }
    ],
    "env": {
      "DEBUG": "False",
      "DJANGO_SETTINGS_MODULE": "socialdots.settings",
      "DATABASE_URL": "sqlite:///:memory:",
      "VERCEL": "1",
      "PYTHONPATH": ".",
      "SECRET_KEY": "django-vercel-deployment-key-change-in-production"
    },
    "functions": {
      "vercel_app.py": {
        "maxDuration": 30
      }
    }
}