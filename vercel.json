{
    "builds": [
      {
        "src": "vercel_app.py",
        "use": "@vercel/python"
      },
      {
        "src": "static/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/vercel_app.py"
      }
    ],
    "env": {
      "DEBUG": "False",
      "DJANGO_SETTINGS_MODULE": "socialdots.settings",
      "DATABASE_URL": "sqlite:///:memory:",
      "VERCEL": "1"
    }
}