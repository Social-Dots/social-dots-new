{
  "version": 2,
  "builds": [
    {
      "src": "socialdots/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "socialdots/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "socialdots.settings",
    "PYTHONPATH": "/var/task"
  }
}